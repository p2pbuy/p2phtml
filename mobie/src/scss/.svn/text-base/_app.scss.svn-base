}

/* card19 */
.card19 {
	font-size: 0;
	letter-spacing: 0;
	word-spacing: 0;
}
.card19 .iconimg {
	margin: 0 0 1rem 0
}
.card19 .box-col {
	position: relative;
	display: inline-block;
	padding: 0.8rem;
	box-sizing: border-box;
}
.card19 .line-separate:nth-child(3n)::before {
	border-right: 0;
	background: none;
}
.card19 .box-col .content-main {
	padding: 5px;
	text-align: center;
}

/* 信息详情 */
