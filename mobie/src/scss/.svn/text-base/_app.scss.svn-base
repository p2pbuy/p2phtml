/* 相机 */
.W_camera {
	width: 320px;
	margin: 0 auto;
	overflow: hidden;
}
.W_camera .m-wrap {
	overflow: hidden;
}
.W_camera ul li {
	display: inline-block;
	float: left;
	margin: 5px 0 0 5px;
}
.W_camera ul li .item,
.W_camera ul li .item .pic {
	display: block;
	width: 100px;
