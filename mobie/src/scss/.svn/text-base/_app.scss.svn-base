/* 选择愿望 */
.wish-box {
	position: relative;
	height: 11.6rem;
	margin-top: 1rem;
	padding: 0.9rem 0 0.9rem 0.9rem;
	overflow: hidden;
}
.wish-box .wish-type {
	width: 9.8rem;
	height: 9.8rem;
	line-height: 9.8rem;
	margin-right: 1.5rem;
	background-image: url(../img/wish-clip.png);
	background-repeat: no-repeat;
	background-size: 98px 298px;
	text-align: center;
	font-size: 1.4rem;
}
.wish-box:before,
.wish-box:after {
	content: " ";
	position: absolute;
	width: 5.6rem;
	height: 5.6rem;
	right: -2.8rem;
	top: -2.8rem;
	-webkit-transform: rotate(-45deg);
}
.wish-box:after {
	text-align: center;
	line-height: 2.6rem;
	font-size: 2rem;
	-webkit-transform: rotate(0);
	color: #fff;
	right: 0;
	top: 0;
	text-indent: 3.4rem;
}
.wish-box.small .wish-type {
	background-position: 0 0;
}
.wish-box.small:before {
	background-color: $wishSmallColor;
}
.wish-box.small:after {
	content: "S";
}
.wish-box.middle .wish-type {
	background-position: 0 -100px;
}
.wish-box.middle:before {
	background-color: $wishMiddleColor;
}
.wish-box.middle:after {
	content: "M";
}
.wish-box.big .wish-type {
	background-position: 0 -200px
}
.wish-box.big:before {
	background-color: $wishBigColor;
}
.wish-box.big:after {
	content: "L";
}
.wish-box .item {
	height: 1.8rem;
	line-height: 1.8rem;
	padding-bottom: 1rem;
	font-family: "Tahoma";
}
.wish-box .item:last-child {
	height: 3.2rem;
}
.wish-box .item .tit {
	display: inline-block;
	width: 5rem;
	text-align: justify;
	vertical-align: middle;
	font-size: 1.5rem;
}
.wish-box .item span {
	display: inline-block;
	vertical-align: middle;
	font-size: 1.5rem;
}
.wish-box .item .W-icon {
	vertical-align: middle;
	margin-right: 0.5rem;
}
.wish-box .btn {
	width: 11.1rem;
	height: 3rem;
	line-height: 3rem;
	border-radius: 2px;
	color: $txtColorA;
}

/* 发现 */
article#main-find {
	padding: 1rem 0 1rem;
}
article#main-find .search-outer {
	margin-bottom: 0.8rem;
	padding: 0.8rem;
	background-color: $baseBarBgA;
