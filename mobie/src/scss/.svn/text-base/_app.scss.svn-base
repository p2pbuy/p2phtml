}

/* 写愿望 */
.wrap-send-wish .content-input {
	margin-bottom: 0.9rem;
}
.wrap-send-wish .content-input textarea {
	width: 100%;
	height: 6rem;
	border: none;
	resize: vertical;
}
.wrap-send-wish .content-input .input-area {
	padding: 1rem;
}
.wrap-send-wish .lbs {
	width: 100%;
	overflow: hidden;
}
.wrap-send-wish .lbs span {
	float: left;
	color: #985a3d;
}
.wrap-send-wish .lbs em {
	float: right;
	color: $txtColorC;
}
.wrap-send-wish .content-input footer {
	text-align: center;
}
.wrap-send-wish .content-input footer a {
	position: relative;
	padding: 0.8rem 0;
}
.wrap-send-wish .reward-input b {
	width: 6.2rem;
	background: #e1b36b;
	padding: 1rem 0;
	text-align: center;
}
.wrap-send-wish .reward-input span input {
	width: 93%;
	height: 5.6rem;
	text-align: center;
	border: none;
}
.wrap-send-wish .reward-input {
	position: relative;
	z-index: 3;
	padding: 0.1rem 0;
}
.wrap-send-wish .wish-type {
	position: relative;
	margin: 0 0.9rem;
}
.wrap-send-wish .wish-type .container {
	position: relative;
	z-index: 2;
	min-height: 2rem;
}
.wrap-send-wish .wish-type .box-shadow {
	height: 1rem;
	background-image: -webkit-gradient(linear, left top, left bottom, color-stop(rgba(218, 218, 218, 0.5)), color-stop(rgba(250, 250, 250, 0.2)));
	background-image: -webkit-linear-gradient(top, rgba(218, 218, 218, 0.5), rgba(250, 250, 250, 0.2));
	background-image: linear-gradient(to bottom, rgba(218, 218, 218, 0.5), rgba(250, 250, 250, 0.2));
}
.wrap-send-wish .wish-type-show .toggle .opt i,
.wrap-send-wish .wish-type-show .toggle .opt .down {
	display: none;
}
.wrap-send-wish .wish-type-hide .toggle .opt .up {
	display: none;
}
.wrap-send-wish .wish-type-hide .card-box {
	display: none;
}
.wrap-send-wish .wish-type .toggle {
	position: absolute;
	left: 50%;
	width: 6rem;
	height: 6rem;
	text-align: center;
}
.wrap-send-wish .wish-type .toggle .inner {
	width: 100%;
	height: 100%;
	position: absolute;
	background: $baseBg;
	left: -50%;
	top: -50%;
	border-radius: 50%;
	box-shadow: 0 0 1px 1px $baselineColor, 0 0 8px 1px rgba(221, 220, 216, 0.5);
}
.wrap-send-wish .wish-type .toggle .opt {
	width: 100%;
	height: 1rem;
	position: absolute;
	z-index: 2;
	background: $baseBg;
	left: -50%;
	top: -14%;
	text-align: center;
	color: $txtColorA;
}
.wrap-send-wish .wish-type .toggle .opt i {
	position: relative;
	top: -0.6rem;
}
.wrap-send-wish .wish-type .toggle .opt b {
	position: relative;
	top: -1.6rem;
}
.wrap-send-wish .wish-type-hide .toggle .opt b {
	top: -0.8rem;
}

/* 发现 轮播 */
.ad-slider-box .ani-loop {
	animation-duration: 15s;
	animation-name: ani-loop;
	animation-iteration-count: infinite;
	animation-fill-mode: both;
	-webkit-animation-duration: 15s;
	-webkit-animation-name: ani-loop;
	-webkit-animation-iteration-count: infinite;
	-webkit-animation-fill-mode: both;
}
@keyframes ani-loop {
	27% {
		margin-left: 0;
	}
	33%,
	60% {
		margin-left: -100%;
	}
	66%,
	97% {
		margin-left: -200%;
	}
	100% {
		margin-left: 0%;
	}
}
@-webkit-keyframes ani-loop {
	27% {
		margin-left: 0;
	}
	33%,
	60% {
		margin-left: -100%;
	}
	66%,
	97% {
		margin-left: -200%;
	}
	100% {
		margin-left: 0%;
	}
}

