}
article#news-detailA-article .extend .media-graphic .mod-media,
article#news-detailA-article .extend .media-graphic .media-main,
article#news-detailA-article .extend .media-graphic img,
article#news-detailB-article .extend .media-graphic .mod-media,
article#news-detailB-article .extend .media-graphic .media-main,
article#news-detailB-article .extend .media-graphic img,
article#news-detailC-article .extend .media-graphic .mod-media,
