	position: relative;
}
.wish-status .stat{
	padding:4px;
	box-shadow: -1px 0 2px -1px #A3A3A3;
	border-radius: 50%;
	position: relative;
	z-index: 5;
	background: $baseCardBgB;
	font-size: 0;
}
.wish-status .stat *{
	vertical-align: middle;
}
.wish-status .box-stat .line-left{
	position: absolute;
	width: 1px;
	height: 100%;
	left: 50%;
	z-index: 3;
}
.wish-status .stat img{
	width:25px;
	height:25px;
	border-radius: 50%;
	display:block;
}
.wish-status .stat-btn{
	display:inline-block;
	width:auto;
	padding:0 10px;
}
#wish-detail-article .card9{
	position:relative;
	z-index: 9;
}