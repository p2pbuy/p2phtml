@charset "utf-8";
/* 主要处理框架元素的布局 */
.card-box {
	margin: 0 0 5px 0;
	
}
article#main-home > .card-box:first-child {
	margin-top: 0.8rem;
}
.card-box.type-gap > div {
	margin-bottom: 8px;
}
.card-box > .title {
	margin: 10px 0 5px 12px;
}
.card-box .more-detail {
	height: 38px;
	text-align: center;
	line-height: 38px;
	margin-top: -1px;
}
@media (-webkit-min-device-pixel-ratio:2), (min-device-pixel-ratio:2) {
	.card-box .more-detail {
		margin-top: 0;
	}
}
.card-box .more-detail a {
	display: block;
	position: relative;
	text-decoration: none;
}
.card-box .more-detail .icon-font-arrow-right {
	margin-left: 3px;
	vertical-align: -2px;
}
.card-box .box-col img {
	width: 100%;
}

/* feed card */
.card-box .card9 {
	margin-bottom: 9px;
	overflow:hidden;
}
.card9 .card {
	margin-bottom: 0;
	margin-top: 7px;
	background-color: #f7f7f7;
}
.card9 .extend-content .card {
	background-color: #fff;
}
.card9 .mod-media {
	margin: 0;
}
.card9 .extend {
	padding: 0 0.8rem 0.8rem;
}
.card9 .card .media-graphic .item-list {
	padding: 5px 0 0 11px;
}
.card9 .operate-inner {
	height: 54px;
}
.card9 .main-content {
	overflow: hidden;
	padding: 8px;
}
.card9 .media-pic {
	text-align: left;
	margin: 5px 0 0 0;
	overflow: hidden;
}
.card9 .media-pic img {
	max-width: 157px;
}
.card9 .media-txt {
	padding: 8px 0 0;
	line-height: 18px;
}
.card9 .media-reward {
	padding: 16px;
	border-left: 3px solid #c1f5e6;
	background-color: #f7f7f7;
	text-align: center;
	line-height: 22px;
}
.card9 .more-detail {
	height: 36px;
	text-align: center;
	line-height: 24px;
	margin-bottom: -1px;
}
.card9 .more-detail a {
	padding: 6px 0;
	text-decoration: none;
}
.card9 .more-detail .icon-font,
.card9 .more-detail .icon {
	vertical-align: -2px;
}
.card9 .more-detail .icon-font-arrow-down {
	margin-left: 3px;
	vertical-align: middle;
}
.card9 .more-detail .num {
	padding: 0 0 0 3px;
	vertical-align: top;
}
.card9 .more-detail .line-separate::after {
	display: none;
}
.card9 .item-main {
	overflow: hidden;
	font-weight: bold;
	line-height: 18px;
}
.card9 .more-detail a {
	padding: 6px 0;
}
.card9 header .item-minor {
	padding: 0;
	line-height: 1rem;
}
.card9 header .media-main:before {
	content: " ";
	position: absolute;
	border: 1px solid #e4e4e4;
	width: 34px;
	height: 34px;
	box-sizing: border-box;
	top: 0;
	left: 0;
	border-radius: 50%;
}
@media (-webkit-min-device-pixel-ratio:2), (min-device-pixel-ratio:2) {
	.card9 header .media-main:before {
		width: 68px;
		height: 68px;
		-webkit-transform: scale(0.5) translate(-50%, -50%);
		-webkit-backface-visibility: hidden;
		-webkit-transform-style: preserve-3d;
		z-index: 2;
		overflow: hidden;
	}
}
.card9 header .mod-media {
	margin-top: 12px;
	margin-bottom: 8px;
}
.card9 .item-minor .from {
	padding: 0 0 0 8px;
}
.card9 .media-graphic {
	padding: 0 0.8rem;
}
.card9 .media-graphic .operate-box {
	position: relative;
	display: block;
	width: 60px;
	padding: 0.8rem 0 0.8rem 0.8rem;
	text-align: right;
}
.card9 .media-graphic .operate-box .btn {
	display: inline-block;
	width: 40px;
	height: 20px;
	line-height: 20px;
	font-size: 1.2rem;
}
.card9 .mod-operate {
	padding: 4px 0;
}
.card9 .operate-box .icon-font {
	position: absolute;
	margin: 0;
	top: 8px;
	right: 8px;
}
.card9 .weibo-detail {
	padding: 4px 12px 10px;
}
.card9 .default-content img,
.card9 .extend-content img {
	vertical-align: text-bottom;
}
.card9 .default-content {
	word-wrap: break-word;
	line-height: 1.8rem;
	margin-bottom: 0.5rem;
}
.card9 .extend-content {
	position: relative;
	margin: 7px -10px -10px;
	background-color: #f7f7f7;
}
.card9 .extend-content .weibo-original {
	line-height: 1.25rem;
}
.card9 .extend-content > .inner {
	padding: 10px;
}
.card9 .head-bar {
	position: relative;
	padding: 10px 0 10px 12px;
	border-top: 0;
	border-left: 0;
	border-right: 0;
	height: 33px;
}
.card9 .head-bar .title {
	color: #e14123;
	margin: 0;
}
.card9 .head-bar .icon-font-arrow-down {
	position: absolute;
	right: 10px;
	top: 10px;
}
.card9 .media-pic-list {
	overflow: hidden;
	width: 248px;
	margin-top: 7px;
}
.card9 .media-pic-list ul {
	margin-top: -5px;
}
.card9 .media-pic-list li {
	float: left;
	width: 76px;
	height: 76px;
	margin: 5px 5px 0 0;
}
.card9 .media-pic-list li img {
	height: 76px;
	width: 76px;
}
.card9 .type-scube {
	width: 170px;
}
.card9 .comment-show {
	background-color: #f7f7f7;
	margin-top: 10px;
	padding: 8px;
}
.card9 .line-gradient {
	height: 24px;
}
.card9 .btn-box {
	height: 34px;
	margin: 8px;
}
.card9 .btn-surl {
	margin-top: -4px;
}
.card9 .mod-media,
.card9 .mod-media img {
	border-radius: 50%;
	overflow: hidden;
}

/* card2 */
.card2 .box-col {
	position: relative;
	height: 28px;
	padding: 7px 0;
	text-align: center;
}
.card2 .mct-s,
.card2 .box-col .icon-font {
